<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Demo1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    
    xmlns:local="clr-namespace:Demo1">
    
    <FlyoutItem >
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />
    </FlyoutItem >

    <TabBar x:Name="main" Route="Main">
        <ShellContent
            Title="Page2"
            ContentTemplate="{DataTemplate local:Page2}"
            Route="Page2" />
        <ShellContent
            Title="Catalog"
            ContentTemplate="{DataTemplate local:CatalogView}"
            Route="CatalogView" />
</TabBar>


    

            

   

</Shell>

